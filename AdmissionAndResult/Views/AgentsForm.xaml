<UserControl x:Class="AdmissionAndResult.Views.AgentsForm"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:telerikReporting="clr-namespace:Telerik.Reporting;assembly=Telerik.Reporting"
             xmlns:tr="http://schemas.telerik.com/wpf"
             Background="White"
             d:DesignHeight="300"
             d:DesignWidth="1014"
             mc:Ignorable="d" >
    <Grid>
        <tr:ReportViewer x:Name="reportViewer" Width="1050"  HorizontalAlignment="Center" VerticalAlignment="Center">
            <tr:ReportViewer.ReportSource>
                <telerikReporting:TypeReportSource TypeName="ProjectReports.MSVerifyingReport, ProjectReports" />
            </tr:ReportViewer.ReportSource>
        </tr:ReportViewer>
    </Grid>
</UserControl>
